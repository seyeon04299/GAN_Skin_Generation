{
  "name": "BCC_Hparam_WGAN",
  "run_name": "_WGAN_BCC+Glr_0.00005+Dlr_0.00005+Gngf_64+Dndf_64+NZ_100",
  "n_gpu": 2,
  "arch_G": {
    "type": "Generator_WGAN",
    "args": {
      "block": "GeneratorBlockWGAN",
      "nz": 100,
      "ngf": 64,
      "G_kernel_size": 4,
      "norm_layer": "BatchNorm2d"
    }
  },
  "arch_D": {
    "type": "Discriminator_WGAN",
    "args": {
      "block": "DiscriminatorBlockWGAN",
      "ndf": 64,
      "D_kernel_size": 4,
      "norm_layer": "BatchNorm2d"
    }
  },
  "hyperparameter_Tuning": true,
  "data_loader": {
    "type": "KNUskinDataLoader_GAN",
    "args": {
      "data_dir": "data/",
      "batch_size": 64,
      "subject_map": "data/KNU_DSLR/KNU_DSLR_subjectmap.csv",
      "cell_type": "BCC",
      "shuffle": false,
      "validation_split": 0.1,
      "num_workers": 2
    }
  },
  "optimizer_G": {
    "type": "RMSprop",
    "args": {
      "lr": 5e-05
    }
  },
  "optimizer_D": {
    "type": "RMSprop",
    "args": {
      "lr": 5e-05
    }
  },
  "metrics": [],
  "lr_scheduler": {
    "type": "StepLR",
    "args": {
      "step_size": 50,
      "gamma": 0.1
    }
  },
  "trainer": {
    "model_name" : "WGAN",
    "epochs": 2000,
    "save_dir": "saved/",
    "save_period": 100,
    "verbosity": 2,
    "monitor": "off",
    "early_stop": 10,
    "tensorboard": true,
    "nz": 100,
    "critic_iter": 5,
    "weight_cliping_limit": 0.01
  }
}
